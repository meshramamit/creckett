package com.creckett.dto;

import java.util.List;

public class UserMatchAmountLeftDTO {
  private UserInfo userInfo;

  private List<MatchAmountInfo> matchAmountInfos;
  
  
  
  public UserInfo getUserInfo() {
	return userInfo;
}


public void setUserInfo(UserInfo userInfo) {
	this.userInfo = userInfo;
}


public List<MatchAmountInfo> getMatchAmountInfos() {
	return matchAmountInfos;
}


public void setMatchAmountInfos(List<MatchAmountInfo> matchAmountInfos) {
	this.matchAmountInfos = matchAmountInfos;
}


public class MatchAmountInfo {
	  private String matchName;
	  
	  private String matchDate;
	  
	  private int crecks;

	public String getMatchName() {
		return matchName;
	}

	public void setMatchName(String matchName) {
		this.matchName = matchName;
	}

	public String getMatchDate() {
		return matchDate;
	}

	public void setMatchDate(String matchDate) {
		this.matchDate = matchDate;
	}

	public int getCrecks() {
		return crecks;
	}

	public void setCrecks(int crecks) {
		this.crecks = crecks;
	}
	  
	  
  }

  
  public class UserInfo{
	  private String userName;
	  
	  private int bankAsset;
	  
	  private int matchesPlayed;
	  
	  private float average;

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public int getBankAsset() {
		return bankAsset;
	}

	public void setBankAsset(int bankAsset) {
		this.bankAsset = bankAsset;
	}

	public int getMatchesPlayed() {
		return matchesPlayed;
	}

	public void setMatchesPlayed(int matchesPlayed) {
		this.matchesPlayed = matchesPlayed;
	}

	public float getAverage() {
		return average;
	}

	public void setAverage(float average) {
		this.average = average;
	}
  }  
}
