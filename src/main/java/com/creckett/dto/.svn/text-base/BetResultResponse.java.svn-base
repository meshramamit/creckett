package com.creckett.dto;

import java.util.Collection;
import java.util.List;

import com.creckett.model.AmountLeft;

public class BetResultResponse {

	private long matchId;
	private long marketId;
	private Collection<UserSessionBets> sessionWiseBets;
	private List<AmountLeft> amountLeft;
	
	public long getMatchId() {
		return matchId;
	}

	public void setMatchId(long matchId) {
		this.matchId = matchId;
	}

	public long getMarketId() {
		return marketId;
	}

	public void setMarketId(long marketId) {
		this.marketId = marketId;
	}

	public void setSessionWiseBets(Collection<UserSessionBets> sessionWiseBets) {
		this.sessionWiseBets = sessionWiseBets;
		
	}

	public Collection<UserSessionBets> getSessionWiseBets() {
		return sessionWiseBets;
	}

	public void setAmountLeft(List<AmountLeft> amountLeft) {
		this.amountLeft = amountLeft;
		
	}

	public List<AmountLeft> getAmountLeft() {
		return amountLeft;
	}
	
	

}
