package com.creckett.dto;

import com.thoughtworks.xstream.annotations.XStreamAlias;
import com.thoughtworks.xstream.annotations.XStreamAsAttribute;

/**
 * POJO representing the ball by ball live score for a given over 
 * @author vkarmani
 *
 */
@XStreamAlias("or")
public class OverRuns {
	
	@XStreamAlias("o")
	@XStreamAsAttribute
	private int overNumber;
	
	@XStreamAlias("b")
   	@XStreamAsAttribute()
   	private int ball=0;
	
	@XStreamAlias("b1")
   	@XStreamAsAttribute
	private String runsInBall1 = "";
	
	@XStreamAlias("b2")
   	@XStreamAsAttribute
	private String runsInBall2 = "";
	
	@XStreamAlias("b3")
   	@XStreamAsAttribute
	private String runsInBall3 = "";
	
	@XStreamAlias("b4")
   	@XStreamAsAttribute
	private String runsInBall4 = "";
	
	@XStreamAlias("b5")
   	@XStreamAsAttribute
	private String runsInBall5 = "";
	
	@XStreamAlias("b6")
   	@XStreamAsAttribute
	private String runsInBall6 = "";
	
	@XStreamAlias("s")
   	@XStreamAsAttribute
	private int session;

	 
	public String getRunsInBall1() {
		return runsInBall1;
	}

	public void setRunsInBall1(String runsInBall1) {
		this.runsInBall1 = runsInBall1;
	}

	public String getRunsInBall2() {
		return runsInBall2;
	}

	public void setRunsInBall2(String runsInBall2) {
		this.runsInBall2 = runsInBall2;
	}

	public String getRunsInBall3() {
		return runsInBall3;
	}

	public void setRunsInBall3(String runsInBall3) {
		this.runsInBall3 = runsInBall3;
	}

	public String getRunsInBall4() {
		return runsInBall4;
	}

	public void setRunsInBall4(String runsInBall4) {
		this.runsInBall4 = runsInBall4;
	}

	public String getRunsInBall5() {
		return runsInBall5;
	}

	public void setRunsInBall5(String runsInBall5) {
		this.runsInBall5 = runsInBall5;
	}

	public String getRunsInBall6() {
		return runsInBall6;
	}

	public void setRunsInBall6(String runsInBall6) {
		this.runsInBall6 = runsInBall6;
	}

	public int getOverNumber() {
		return overNumber;
	}

	public void setOverNumber(int overNumber) {
		this.overNumber = overNumber;
	}

	public int getBall() {
		return ball;
	}

	public void setBall(int ball) {
		this.ball = ball;
	}

	public int getSession() {
		return session;
	}

	public void setSession(int session) {
		this.session = session;
	}
	
	
	
	
}
